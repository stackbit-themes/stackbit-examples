        "@types/react": "^18.2.79",
        "@types/react-dom": "^18.2.25",
        "autoprefixer": "^10.4.19",
        "contentful-export": "^7.19.44",
        "contentful-import": "^8.5.42",
        "gatsby-plugin-postcss": "^6.13.1",
